<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="OsteoMAUIApp.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:OsteoMAUIApp"
    xmlns:profile="clr-namespace:OsteoMAUIApp.Views.ProfileSettings"
    Shell.FlyoutBehavior="Disabled"
    Title="OsteoMAUIApp">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />

    <ShellContent
    Title="Profile Settings"
    ContentTemplate="{DataTemplate profile:ProfileIndex}"
    Route="ProfileIndex" />
</Shell>
